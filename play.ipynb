    "import math\n",
    "\n",
    "def gap_duration_score(G_d, M):\n",
    "    if G_d <= M:\n",
    "        return 1 + 1 / (1 + math.exp(0.05 * (G_d - M / 2)))\n",
    "    else:\n",
    "        return M / G_d"
   ]
  },
